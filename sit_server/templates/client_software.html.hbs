{{> main-top title="Software"}}
<div class="software-table">
    <div class="filters">

    </div>
    <table>
        <thead>
        <tr>
            <th>Name</th>
            <th>Version</th>
            <th>Hersteller</th>
        </tr>
        </thead>
        <tbody>
        {{#each software}}
            <tr>
                <td><a href="/software/{{this.[2].[1].id}}">{{this.[2].[1].name}}</a></td>
                <td><a href="/software/version/{{this.[2].[0].id}}">{{this.[2].[0].version}}</a></td>
                <td>{{this.[2].[1].publisher}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
{{> main-bottom}}