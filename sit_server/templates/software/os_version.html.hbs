{{> _layout_software-top title="Betriebssystem"}}
<div class="os-table">
    <h1>{{os_name}}</h1>
    <p><a href="/software/os/{{os_name}}/computer">Computer-Liste</a></p>
    <div class="filters">

    </div>
    <table>
        <thead>
        <tr>
            <th>Version</th>
            <th>Anzahl</th>
        </tr>
        </thead>
        <tbody>
        {{#each os_version}}
            <tr>
                <td><a href="/software/os/{{../os_name}}/version/{{this.os_version}}">{{this.os_version}}</a></td>
                <td>{{this.count}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
{{> _layout_software-bottom}}